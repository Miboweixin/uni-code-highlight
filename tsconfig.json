{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "esnext"
    ],
    "outDir": "dist",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}